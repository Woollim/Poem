<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="root.hash_tm">

    <uses-permission android:name="android.permission.BLUETOOTH" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.SignInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity android:name=".activity.NoOutPoemActivity"/>
        <activity android:name=".activity.MainActivity"/>
        <activity android:name=".activity.SignUpActivity"/>
        <activity android:name=".activity.MyPageActivity"/>
        <activity android:name=".activity.PoemtryIndexActivity"/>
        <activity android:name=".activity.WritePoemActivity"/>
        <activity android:name=".activity.PoemActivity"/>
        <activity android:name=".activity.MakePoemtryActivity"/>
        <activity android:name=".activity.BluetoothShareActivity"/>

    </application>

    <uses-permission android:name="android.permission.INTERNET"/>

</manifest>